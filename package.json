{
  "name": "mip_server",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts",
    "rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "latest": "knex --knexfile knexfile.ts migrate:latest",
    "reset": "knex --knexfile knexfile.ts migrate:rollback && knex --knexfile knexfile.ts migrate:latest && ts-node-dev --transpile-only --ignore-watch node_modules src/server.ts"
  },
